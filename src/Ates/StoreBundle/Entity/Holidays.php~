<?php
namespace Ates\StoreBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * @ORM\Entity
 * @ORM\Table(name="neradni_dani")
 */
class Holidays
{
    /**
    * @ORM\Id
    * @ORM\Column(type="integer")
    * @ORM\GeneratedValue(strategy="AUTO")
    */
    protected $id;
    
    /**
    * @ORM\Column(type="integer")
    */
    protected $id_admin;
    
    /**
    * @ORM\Column(type="date")
    */
    protected $date;
    
   /**
     * @ORM\Column(type="string", length=100)
     */
    protected $name;

   
}